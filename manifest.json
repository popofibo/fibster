{
  "manifest_version": 2,
  "name": "Fibster",
  "description": "Movie API",
  "version": "1.1",
  "browser_action": {
  "defaut_popup":"popup.html"
  },
   
  "background":{
    "page":"background.html"
  },
  
  "browser_action": {
    "default_icon": "static/icon.png"
  },
  
  "icons": {
    "16": "static/icon_bitty.png",
	"48": "static/icon_small.png",
	"128": "static/icon_large.png"
  },
  
  "content_scripts": [
    {
	  "js": ["js/myscript.js", "js/dialog.js"],
      "matches": ["<all_urls>"]
    }
  ],
  
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ]
}